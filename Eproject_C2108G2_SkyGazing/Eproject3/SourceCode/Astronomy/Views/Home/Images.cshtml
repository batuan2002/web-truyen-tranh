
@{
    ViewBag.Title = "Images";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@{ 
    List<string> images = ViewBag.listimage;
}
<h2>Images</h2>

<div class="container">
    <div class="container-fluid">
        <!--Images-->
        @foreach (string item in images)
        {
            <img class="ima" src="@Url.Content(item)" />
        }
    </div>
    
</div>
<style>
    .container-fluid {
        padding-left: 3%;
    }

    .ima {
        width: 24%;
        height: 250px;
    }
</style>